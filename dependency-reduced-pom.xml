      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.19.1</version>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <manifest>
              <mainClass>com.diff.filesdiff.FilesDiffApplication</mainClass>
            </manifest>
          </archive>
        </configuration>
      </plugin>
